{% extends 'base.html.twig' %}
{% block stylesheets %}
{{ parent() }}
    <link href="{{ asset('/bundles/framework/css/bootstrap.css') }}" rel="stylesheet" media="screen">
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/validationEngine.jquery.css') }}"/>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootswatch/3.3.6/flatly/bootstrap.min.css"/>
    <link href="{{ asset('/bundles/framework/css/styles.css') }}" rel="stylesheet" />
{% endblock %}
{% block body %}
    {% include 'header.html.twig' %}
    <div class="container-fluid">
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <div class="panel panel-default">
                        <div class="panel-heading"><img src="{{ asset('images/timetable.png') }}"></div>
                        <div class="panel-body">
                            {{ form_start(form) }}
                            {{ form_widget(form) }}
                            {{ form_end(form) }}
                            <div class="works"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
    <div class="modal"></div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {#<script src="{{ asset('bundles/framework/js/jQuery/jquery-1.12.3.min.js') }}"></script>
    <script src="{{ asset('bundles/framework/js/jQuery/jquery.ValidationEngine.js') }}"></script>
    <script src="{{ asset('bundles/framework/js/jQuery/jquery.ValidationEngine-en.js') }}"></script>#}
    <script src="{{ asset('bundles/framework/js/schedule.js') }}"></script>
{% endblock %}
